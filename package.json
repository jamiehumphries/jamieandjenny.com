{
  "name": "jamieandjenny.com",
  "version": "1.0.0",
  "description": "Jamie and Jennyâ€™s wedding website",
  "author": "Jamie Humphries",
  "license": "ISC",
  "private": "true",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "run-p build:*",
    "build:sass": "npm run sass -- --style=compressed --no-source-map",
    "clean": "rimraf src/public/generated/",
    "predev": "npm run clean",
    "dev": "cross-env NODE_ENV=development run-p dev:*",
    "dev:sass": "npm run sass -- --watch",
    "dev:server": "node-dev --inspect --respawn src/server.js",
    "dev:sync": "browser-sync start --config bs-config.js",
    "lint": "eslint src/",
    "sass": "sass src/styles/:src/public/generated/css/",
    "start": "node src/server.js"
  },
  "dependencies": {
    "express": "4.18.1",
    "express-sslify": "1.2.0",
    "nunjucks": "3.2.3"
  },
  "devDependencies": {
    "browser-sync": "2.27.10",
    "cross-env": "7.0.3",
    "eslint": "8.16.0",
    "eslint-config-prettier": "8.5.0",
    "node-dev": "7.4.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.6.2",
    "rimraf": "3.0.2",
    "sass": "1.52.1"
  }
}
